{
  "name": "service-config-leader",
  "version": "1.0.11",
  "description": "A service for centralization of Pryv.io configuration and distribution among all Pryv.io components.",
  "main": "index.js",
  "author": "Thi√©baud Modoux",
  "license": "MIT",
  "scripts": {
    "start": "node_modules/.bin/babel-node src/server.js --config dev-config.json  --platformConfig platform.yml",
    "start-prod": "node dist/server.js --config dev-config.json",
    "release": "node_modules/.bin/babel src/ -d dist/",
    "watch": "node_modules/.bin/babel --verbose --watch=./src --out-dir=dist/ --copy-files --source-maps both",
    "test": "node_modules/.bin/mocha --require babel-core/register --require module-alias/register --recursive test/ --exit",
    "test-acceptance": "node_modules/.bin/mocha --require babel-core/register --require module-alias/register --recursive test/acceptance/ --exit",
    "lint": "./node_modules/.bin/eslint ./src ./test"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^7.1.0",
    "bluebird": "^3.7.2",
    "compare-versions": "^3.6.0",
    "cron": "^1.8.2",
    "crypto": "^1.0.1",
    "crypto-random-string": "^3.2.0",
    "express": "^4.17.1",
    "express-joi-validation": "^4.0.3",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "nconf": "^0.10.0",
    "nconf-yaml": "^1.0.2",
    "winston": "^2.3.0"
